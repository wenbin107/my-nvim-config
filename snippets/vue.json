{
  "setup": {
    "prefix": ["setup", "template"],
    "body": [
      "<template>",
      "\t$1",
      "</template>",
      "",
      "<script lang=\"ts\" setup>",
      "\t$2",
      "</script>",
      "",
      "<style lang=\"sass\">",
      "\t$3",
      "</style>",
      ""
    ],
    "description": "My standard setup Vue3 + TS"
  },
  "template" : {
     "prefix" : ["template"],
     "body": [
        "<template>",
        "\t$1",
        "</template>",
        "",
        "<script>",
        "\t export default {",
        "\t}",
        "</script>",
        "",
        "<style lang=\"sass\" scoped>",
        "\t$3",
        "</style>"
     ],

    "description": "My standard template Vue2"
  }
}
